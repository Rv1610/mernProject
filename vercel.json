{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "backend/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/submit-form",
        "dest": "backend/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1",
        "status": 200
      }
    ]
    
  }
  